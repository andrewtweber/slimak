{
    "name": "andrewtweber/slimak",
    "type": "library",
    "description": "Slugged models for Laravel",
    "keywords": [
        "laravel",
        "slugs",
        "slugged",
        "models"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Weber",
            "homepage": "https://andrew.cool"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-intl": "*",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "larastan/larastan": "^3.6",
        "orchestra/testbench": "^10.4",
        "phpunit/phpunit": "^11"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Slimak\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slimak\\Tests\\": "tests/"
        }
    }
}

